{
  "name": "michel-psi-webpage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "npm run clean && rm -rf node_modules && npm install",
    "update:deps": "npx npm-check-updates -u && npm install",
    "check:deps": "npx npm-check-updates",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:restart": "docker-compose restart",
    "docker:build": "docker-compose build",
    "docker:rebuild": "docker-compose down && docker-compose build --no-cache && docker-compose up -d",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:db": "docker-compose logs -f db",
    "docker:logs:pgadmin": "docker-compose logs -f pgadmin",
    "docker:clean": "docker-compose down -v --remove-orphans && docker system prune -f",
    "docker:status": "docker-compose ps",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset --force",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:format": "prisma format",
    "db:validate": "prisma validate",
    "db:pull": "prisma db pull",
    "setup": "npm run docker:up && sleep 10 && npm run db:push && npm run db:generate && npm run db:seed",
    "setup:dev": "npm run docker:up && sleep 10 && npm run db:migrate && npm run db:seed",
    "setup:fresh": "npm run docker:down && npm run docker:up && sleep 15 && npm run db:migrate:reset && npm run db:seed",
    "dev:full": "npm run docker:up && npm run db:generate && npm run dev",
    "reset": "npm run docker:down && npm run docker:clean && npm run setup"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.4",
    "@sendgrid/mail": "^8.1.5",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "googleapis": "^148.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.446.0",
    "multer": "^2.0.0",
    "next": "14.2.13",
    "react": "^18",
    "react-day-picker": "9.6.7",
    "react-dom": "^18",
    "react-icons": "^5.4.0",
    "react-image-gallery": "^1.3.0",
    "react-quill": "^2.0.0",
    "recharts": "^2.15.3",
    "sharp": "^0.34.2",
    "swiper": "^11.1.15",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/date-fns": "^2.6.3",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18.3.20",
    "@types/react-datepicker": "^7.0.0",
    "@types/react-dom": "^18",
    "@types/react-image-gallery": "^1.2.4",
    "eslint": "^8",
    "eslint-config-next": "14.2.13",
    "postcss": "^8",
    "prisma": "^5.20.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
